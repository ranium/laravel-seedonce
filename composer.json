{
    "name": "ranium/laravel-seedonce",
    "description": "Laravel package to run seeders only once",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.1"
    },
    "autoload": {
        "psr-4": {
            "Ranium\\SeedOnce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ranium\\SeedOnce\\Test\\": "tests"
        },
        "classmap": [
            "tests/database/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ranium\\SeedOnce\\SeedOnceServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Abbas Ali",
            "email": "abbas@ranium.in",
            "homepage": "https://ranium.in",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
      "test": [
        "./vendor/bin/phpunit"
      ]
    }
}
